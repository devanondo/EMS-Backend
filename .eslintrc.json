{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "eslint:recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0
  }
}
